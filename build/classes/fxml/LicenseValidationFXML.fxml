<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="validateLicenseAnchorPane" fx:id="validateLicenseAnchorPane" prefHeight="140.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sandhata.gcat.application.controller.LicenseValidationController">
    <!--
<stylesheets>
    <URL value="@/com/sandhata/gcat/application/css/licensevalidationfxml.css" />
</stylesheets>
    -->
    <children>
        <HBox layoutX="184.0" layoutY="58.0" prefHeight="25.0" prefWidth="345.0">
            <children>
                <TextField id="tfvalidateLicense" fx:id="tfvalidateLicense" editable="false" prefHeight="25.0" prefWidth="283.0" promptText="please upload a valid license" />
                <Button id="bvalidateLicense" fx:id="bvalidateLicense" mnemonicParsing="false" onAction="#browseLicenseClicked" prefHeight="25.0" prefWidth="64.0" style="-fx-background-color: #95adbe;" text="Browse" />
            </children>
        </HBox>
        <Label layoutX="83.0" layoutY="62.0" text="GCAT License    :" />
      <Button id="bUpload" fx:id="bUpload" layoutX="274.0" layoutY="101.0" mnemonicParsing="false" onAction="#uploadButtonClicked" style="-fx-background-color: #95adbe;" text="Upload" />
    </children>
</AnchorPane>
